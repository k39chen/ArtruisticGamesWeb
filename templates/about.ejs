<%
var links = !_.isUndefined(this.links) ? this.links : {};
var message = !_.isUndefined(this.message) ? this.message : null;
var members = !_.isUndefined(this.members) ? this.members : [];
%>
<h1>About</h1>
<div class="content">
    <div class="services">
        <a class="fa fa-youtube-square" href="<%=links.YouTube%>" target="_blank"></a>
        <a class="fa fa-facebook-square" href="<%=links.Facebook%>" target="_blank"></a>
        <a class="fa fa-twitter-square" href="<%=links.Twitter%>" target="_blank"></a>
        <a class="fa fa-tumblr-square" href="<%=links.Tumblr%>" target="_blank"></a>
        <a class="fa fa-rss-square" href="<%=links.RSS%>" target="_blank"></a>
    </div>
</div>
<div class="text">
    <%=message%>
</div>
<div class="team-members">
<% _.each(members, function(member, index) { %>
    <span class="team-member">
        <div class="wrapper">
            <img class="picture" src="<%=member.picture%>" />
            <div class="name"><%=member.name%></div>
            <div class="role"><%=member.role%></div>
        </div>
        <div class="mask"></div>
    </span>
<% }); %>
</div>
